parameters:
  level: 'max'
  paths:
    - bin/rr-worker
    - config
    - fixes
    - helpers
    - src
  reportUnmatchedIgnoredErrors: true
  ignoreErrors:
    - message: '#Call to protected method bootstrappers\(\) of class.+Kernel#'
      paths: [src/Application/Factory.php]
    - message: '#Access to an undefined static property.+Cache#'
      paths: [src/Listeners/FlushStrCacheListener.php]
